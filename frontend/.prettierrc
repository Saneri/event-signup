{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "printWidth": 120,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/.*$",
    "^(?!.*[.](css|svg)$)[./].*$",
    "",
    ".css$",
    ".svg$"
  ]
}
